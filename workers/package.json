{
  "name": "@simbi/workers",
  "version": "2.0.0",
  "description": "Polyglot worker system for Simbi - supports Ruby, Python, and TypeScript",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node start.ts",
    "workers:start": "ts-node start.ts",
    "workers:dev": "nodemon --exec ts-node start.ts",
    "test": "jest",
    "test:workers": "jest --testMatch='**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "build": "tsc"
  },
  "keywords": [
    "workers",
    "queue",
    "bull",
    "redis",
    "polyglot",
    "elide"
